{
  "window.zoomLevel": 1.5,
  "workbench.tree.indent": 18,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.colorTheme": "Tokyo Night",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.sideBar.location": "right",
  "workbench.sideBar.width": 360,
  "workbench.activityBar.location": "bottom",
  "workbench.statusBar.visible": true,
  "workbench.layoutControl.enabled": false,
  "workbench.reduceMotion": "on",
  "editor.fontFamily": "FiraCode Nerd Font, JetBrainsMono Nerd Font, CaskaydiaCove Nerd Font, monospace",
  "editor.fontSize": 18,
  "editor.lineHeight": 1.3,
  "editor.letterSpacing": 0.2,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.smoothScrolling": true,
  "editor.mouseWheelScrollSensitivity": 0.9,
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "selection",
  "editor.minimap.enabled": true,
  "editor.stickyScroll.enabled": true,
  "editor.scrollBeyondLastLine": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.tabSize": 4,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.cursorStyle": "block",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.scrollback": 10000,
  "window.commandCenter": false,
  "window.autoDetectHighContrast": false,
  "workbench.list.smoothScrolling": true,
  "window.titleBarStyle": "custom",
  "editor.accessibilitySupport": "off",
  "editor.hover.delay": 200,
  "editor.inlayHints.enabled": "off",
  "editor.bracketPairGuides": "active",
  "editor.renderLineHighlight": "all",
  "workbench.colorCustomizations": {
    "sideBar.background": "#1a1b26",
    "sideBar.foreground": "#c0caf5",
    "activityBar.background": "#16161e",
    "activityBar.foreground": "#7aa2f7",
    "statusBar.background": "#16161e",
    "statusBar.foreground": "#a9b1d6"
  },
  "workbench.startupEditor": "none",
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "files.exclude": {
    "**/.git": true,
    "**/.cache": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/build": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "C_Cpp_Runner.msvcBatchPath": "",
  "C_Cpp_Runner.cCompilerPath": "gcc",
  "C_Cpp_Runner.cppCompilerPath": "g++",
  "C_Cpp_Runner.debuggerPath": "gdb",
  "C_Cpp_Runner.cStandard": "",
  "C_Cpp_Runner.cppStandard": "",
